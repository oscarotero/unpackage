{
  "lock": false,
  "tasks": {
    "test": "deno test --allow-net test.ts",
    "update-deps": "deno run -A --quiet 'https://deno.land/x/nudd@v0.2.11/cli.ts' update '**/*.ts' deno.json"
  },
  "fmt": {
    "exclude": [
      "CHANGELOG.md"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-import-prefix"
      ]
    }
  }
}
